{
  "name": "rolldown-plugin-unocss",
  "version": "0.1.0",
  "author": "subframe7536",
  "description": "A Rolldown/tsdown plugin for UnoCSS using @unocss/core",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/subframe7536/rolldown-plugin-unocss#readme",
  "bugs": {
    "url": "https://github.com/subframe7536/rolldown-plugin-unocss/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subframe7536/rolldown-plugin-unocss.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "release": "bun run build && bumpp"
  },
  "peerDependencies": {
    "@unocss/core": "*",
    "@unocss/config": "*",
    "magic-string": "*"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "bumpp": "^10.3.2",
    "tsdown": "^0.17.4",
    "typescript": "^5.9.3"
  }
}